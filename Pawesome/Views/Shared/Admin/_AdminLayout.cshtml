<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Administration Pawesome</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap"
        rel="stylesheet">
    <link
        href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="~/css/base/reset.css"/>
    <link rel="stylesheet" href="~/css/base/variables.css"/>
    <link rel="stylesheet" href="~/css/base/typography.css"/>
    <link rel="stylesheet" href="~/css/components/buttons.css"/>
    <link rel="stylesheet" href="~/css/components/badges.css"/>
    <link rel="stylesheet" href="~/css/components/alerts.css"/>
    <link rel="stylesheet" href="~/css/components/avatar.css"/>
    <link rel="stylesheet" href="~/css/components/forms.css"/>
    <link rel="stylesheet" href="~/css/layouts/navbar.css"/>
    <link rel="stylesheet" href="~/css/layouts/container.css"/>
    <link rel="stylesheet" href="~/css/pages/admin.css"/>
    <link rel="stylesheet" href="~/Pawesome.styles.css" asp-append-version="true"/>
</head>
<body>
<header class="container">
    <partial name="_NavBar"/>
</header>
<main role="main" class="container">
    @RenderBody()
</main>
<partial name="_Footer"/>

<script src="~/lib/jquery/jquery.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/lib/microsoft/signalr/dist/browser/signalr.min.js"></script>
<script src="~/js/notificationClient.js"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
